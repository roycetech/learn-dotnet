@model ForgingAhead.Models.Quest
<h2>
  Create Quest
</h2>

<form  >
  <div>
    <h4>Quest</h4>
    <hr>
        <div class="row">
            <label class="control-label col-md-2" asp-for="Name"></label>
            <div class="col-md-10">
                <input class="form-control" asp-for="Name" />
            </div>
        </div>
        <div class="row">
            <label class="control-label col-md-2" asp-for="IsActive"></label>
            <div class="col-md-10">
                <input class="form-control" asp-for="IsActive" />
            </div>
        </div>
    <div>
      <input type="submit" value="Create" class="btn btn-default"/>
    </div>
  </div>
</form>

